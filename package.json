{
  "private": true,
  "workspaces": [
    "frontend",
    "api"
  ],
  "scripts": {
    "start": "concurrently \"yarn start:fe\" \"yarn start:be\"",
    "start:fe": "yarn workspace emeal-frontend run react-scripts start",
    "build:fe": "yarn workspace emeal-frontend run react-scripts build",
    "start:be": "yarn workspace emeal-backend run dev",
    "build:be": "yarn workspace emeal-backend run build",
    "build:modal": "yarn workspace emeal-frontend run build:modal"
  }
}
